<Context>
	<Resource auth="Container"
	    driverClassName="com.mysql.jdbc.Driver"
		name="jdbc/mydb"
		type="javax.sql.DataSource"
		url="jdbc:mysql://192.168.0.89:3308/supermercado"
	   	username="supermercado"
		password="supermercado"
       logAbandoned="true"
       maxActive="50"
       	maxIdle="5"
       	minIdle="1"
